{
  "name": "ai-shortx-client",
  "private": true,
  "version": "0.1.0",
  "description": "AIShortX客户端",
  "author": "AI ShortX Team",
  "type": "module",
  "main": "public/electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm run react:dev\" \"npm run electron:dev\"",
    "react:dev": "vite",
    "electron:build": "tsc -p electron && node scripts/build-electron.js",
    "electron:dev": "wait-on tcp:5174 && npm run electron:build && electron .",
    "build": "tsc -b && vite build && npm run electron:build",
    "build:backend": "cd ../backend && npm run build",
    "dist": "npm run build:backend && npm run build && electron-builder --config electron-builder.config.cjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^6.0.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "cos-js-sdk-v5": "^1.10.1",
    "dayjs": "^1.11.19",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "electron-is-dev": "^3.0.1",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
